#include <stdio.h>
void display(int arr[],int len)
{  int sum=0;
  for (int i=0;i<len;i++){
      printf("%d",arr[i]);
      sum+=arr[i];
  }printf("\n %d",sum);
}
int main()
{
   int size ;
   printf("enter a number:");
   scanf("%d",&size );
 int  arr[size];
 for (int i=0;i<size;i++)
 {
     scanf("%d",&arr[i]);
 }
 display(arr,size);
 

   
    return 0;
}
